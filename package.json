{
  "name": "tinglebuzz",
  "version": "1.0.0",
  "description": "Realtime informational assistance.",
  "main": "index.js",
  "scripts": {
    "test": "mocha src/**/*.test.js",
    "test:watch": "npm run test -- --watch",
    "lint": "NODE_ENV=development ./node_modules/.bin/esw --color --env browser,node ./src ./tools",
    "lint:watch": "npm run lint -- -w",
    "dev": "npm-run-all lint:watch build:dev",
    "build:dev": "NODE_ENV=development babel-node ./tools/start.js",
