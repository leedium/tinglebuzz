    "url": "https://github.com/leedium/tinglebuzz/issues"
  },
  "homepage": "https://github.com/leedium/tinglebuzz#readme",
  "devDependencies": {
    "babel-loader": "^6.2.9",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "colors": "^1.1.2",
    "copy": "^0.3.0",
    "cp": "^0.2.0",
    "del": "^2.2.2",
    "enzyme": "^2.6.0",
    "eslint": "^3.12.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
